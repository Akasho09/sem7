   "source": [
    "from sklearn.naive_bayes import GaussianNB\n",
    "from sklearn.datasets import load_iris\n",
    "\n",
    "dataset = load_iris()\n",
    "X = dataset.data\n",
    "Y = dataset.target\n",
    "\n",
    "model = GaussianNB()\n",
    "model.fit(X,Y)\n",
    "\n",
    "print(\"Prediction : \" , model.predict([[1.2 , 3 , 4 , 5.1]]))"
   ]
  }
